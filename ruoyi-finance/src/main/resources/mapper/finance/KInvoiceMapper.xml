            <if test="cwInvoiceMoney != null">#{cwInvoiceMoney},</if>
            <if test="cwApplicationDate != null">#{cwApplicationDate},</if>
            <if test="cwInvoiceDate != null">#{cwInvoiceDate},</if>
            <if test="cwInvoiceStatus != null">#{cwInvoiceStatus},</if>
